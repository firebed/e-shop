{
  "name": "firebed/eshop",
  "description": "Complete eshop framework using Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "okang",
      "email": "okan.giritli@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "laravel/framework": "^8.0",
    "livewire/livewire": "^2.3",
    "barryvdh/laravel-dompdf": "^0.9.0",
    "firebed/bootstrap-components": "dev-master",
    "firebed/permission": "dev-master",
    "intervention/image": "^2.5",
    "laravel/cashier": "^12.13",
    "maatwebsite/excel": "^3.1",
    "paypal/paypal-checkout-sdk": "1.0.1",
    "stevebauman/location": "^6.1",
    "laravel/fortify": "^1.7",
    "firebed/sitemap": "dev-master"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Eshop\\": "src/",
      "Eshop\\Database\\Factories\\": "database/factories/",
      "Eshop\\Database\\Seeders\\": "database/seeders/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Eshop\\EshopServiceProvider",
        "Eshop\\CartServiceProvider",
        "Eshop\\LivewireServiceProvider",
        "Intervention\\Image\\ImageServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}

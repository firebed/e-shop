{
  "name": "firebed/eshop",
  "description": "Complete eshop framework using Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "okang",
      "email": "okan.giritli@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^8.0",
    "livewire/livewire": "^2.3",
    "firebed/bootstrap-components": "dev-master",
    "firebed/permission": "^1.0",
    "firebed/aade-mydata": "^1.0",
    "intervention/image": "^2.5",
    "laravel/cashier": "^12.13",
    "maatwebsite/excel": "^3.1",
    "paypal/paypal-checkout-sdk": "1.0.1",
    "stevebauman/location": "^6.1",
    "laravel/fortify": "^1.7",
    "firebed/sitemap": "dev-master",
    "firebed/laravel-scout-database": "dev-master",
    "hisorange/browser-detect": "^4.4",
    "picqer/php-barcode-generator": "^2.2",
    "ext-simplexml": "*",
    "ext-intl": "*",
    "ext-soap": "*",
    "ext-dom": "*",
    "barryvdh/laravel-dompdf": "^2.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "orchestra/testbench": "^6.21",
    "phpunit/phpunit": "^9.5",
    "barryvdh/laravel-debugbar": "^3.6"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "Eshop\\": "src/",
      "Eshop\\Database\\Factories\\": "database/factories/",
      "Eshop\\Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Eshop\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
